    <category term="正定矩阵" scheme="https://panqiincs.me/tags/%E6%AD%A3%E5%AE%9A%E7%9F%A9%E9%98%B5/"/>
    
    <category term="内积空间" scheme="https://panqiincs.me/tags/%E5%86%85%E7%A7%AF%E7%A9%BA%E9%97%B4/"/>
    
